ccode use "stdio.h";
ccode use "conio.h";

function ReadStr as string
    var temp : string ;
    ccode "scanf(D\"%s\"D,&temp);";
    return temp;
end;

function ReadInt as int
    var temp : int ;
    ccode "scanf(D\"%d\"D,&temp);";
    return temp;
end;

function write(s1 string)
    ccode "printf(D\"%s\"D,s1);";
end;

